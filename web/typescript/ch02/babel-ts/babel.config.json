{
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": "usage",
          "corejs": {
            "version": 3,
            "proposals": true
          }
        }
      ],
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-arrow-functions",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }